/* pages/nodata/nodata.wxss */
.img{
  width: 135px;
  height: 135px;
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translateX(-50%);
}
.img image{
  width: 100%;
  height: 100%;
}
.text{
  width: 100%;
  text-align: center;
  color: #969798;
  font-size: 16px;
  position: absolute;
  top: calc(33% + 135px);
}
.btn{
  width: 40%;
  border: 1px solid #ccc;
  border-radius: 30px;
  color: #969798;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: absolute;
  top: calc(40% + 135px);
  left: 50%;
  transform: translateX(-50%);
  background: #fff;
}