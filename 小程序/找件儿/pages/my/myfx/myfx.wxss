/* pages/my/myfx/myfx.wxss */
.listwrap{
  position: relative;
  height: 68px;
  margin-top:10px; 
}
.list{
  width: 100%;
  height: 100%;
  position: absolute;
  padding: 10px 15px;
  z-index: 100;
  box-sizing: border-box;
  background: #fff;
}
.bottom{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  box-shadow: 1px 1px 8px #ccc;
}
.bottom .left,.bottom .right{
  float: left;
  height: 100%;
  position: relative;
}
.bottom .left{
  width: 70%;
}
.bottom .left .tj,.bottom .left .qk{
  position: absolute;
  top: 50%;
  transform: translateY(-50%); 
}
.bottom .left .tj{
  color: #4A7BB7;
  left: 20%;
}
.bottom .left .qk{
  color: #F0A42D;
  right: 20%;
}
.bottom .left text{
  border:1px solid #ccc;
  color:#ccc;
  width:20px;
  height:20px;
  display:inline-block;
  text-align:center;
  line-height:18px;
  margin-right:5px;
  border-radius: 50%;
}
.list .name{
  color: #6785AD;
  font-size: 18px;
}
.list .oe{
  color: #B1B2B2;
  font-size: 16px;
}
.list .money{
  color: #DA2323;
  font-size: 16px;
  position: absolute;
  right: 20px;
  top: 10px;
}
.delete{
  height: 100%;
  width: 90px;
  position: absolute;
  right: 0;
  top: 0;
  background: #fff;
  z-index: 90;
  background: #EFF3FA;
}
.delete image{
  width: 20px;
  height: 26px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}


.bottom .right{
  width: 30%;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  background: #329BDE;
}