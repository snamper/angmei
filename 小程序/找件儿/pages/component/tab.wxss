/* pages/component/tab.wxss */
.tabwrap{
	background: #FFFFFF;
	position: fixed;
	bottom: 0;
	width: 100%;
  box-shadow: 1px 1px 8px #ccc;
  border-top: 1px solid #ccc;
}
.tab{
	float: left;
	width: 33%;
	text-align: center;
	font-size: 14px;
	color: #54a1ec;
	padding: 10rpx 0;
}
.tab image{
	width: 60rpx;
	height: 60rpx;	
}