/* pages/inde/inde.wxss */
.tab{
  display: flex;
  margin-top: 20rpx;
}
.tabFlex{
  width: 230rpx;
  margin-left: 15rpx;
}
.tabFlex image{
  width: 230rpx;
}
.content{
  padding: 0 15rpx;
  margin-top: 20rpx;
}
.banner2{
  width: 720rpx;
}
.introduce{
  background: url('data:image/jpeg;base64,') center center / 100% 100%  no-repeat;
  height: 700rpx;
  padding:0 40rpx;
  overflow: hidden;
}
.introduce view{
  font-size: 30rpx;
  margin-top: 140rpx;
  line-height: 54rpx;
}
.productWrap{
  margin-top: 60rpx;
}
.productWrap .productimg{
  width: 720rpx;
}
.productBannerWrap{
  display: flex;
  flex-wrap: wrap;
  justify-content:space-around;
  padding-bottom: 40rpx;
}
.productBanner{
   width: 320rpx;
   border-radius: 20rpx;
   background: #fff;
   box-shadow: 1px 1px 8px #ccc;
   margin-top: 40rpx;
   text-align: center;
   padding-top:80rpx; 
   height: 280rpx;
   overflow: hidden;
   position: relative;
}
.productBanner image{
  width: 190rpx;
}
.productBanner:nth-child(3),.productBanner:nth-child(4){
  padding-top: 100rpx;
}
.productBanner:nth-child(3) image,.productBanner:nth-child(4) image{
  width: 230rpx;
}
.productBanner .title{
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  background: #60d634;
  color: #FFF;
  position: absolute;
  bottom: 0;
  width: 100%;
}